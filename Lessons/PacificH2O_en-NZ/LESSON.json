        "lesson": [
          {
            "lsnNum": 1,
            "lsnTitle": "Water Matters: Mind-mapping the meaning of water",
            "lsnDur": 45,
            "lsnPreface": "Students create mind maps to reflect on what water means to them, as an engagement hook to investigate water purification technologies and Pacific water issues.",
            "learningObj": [
              "Explore their personal values and experiences related to water and connect them to the importance of engineering clean water in the Pacific.",
              "Examine the value of water through a Pacific cultural lens."
            ],
            "lsnPrep": {
              "prepTitle": "Prep",
              "prepDur": 10,
              "prepQuickDescription": "Print student handouts and gather pens/pencils/markers for mind-mapping. Make sure your projector can play audio and video included in the slideshow.",
              "prepDetails": null,
              "prepVariantNotes": null,
              "prepTeachingTips": null
            },
            "chunks": [
              {
                "chunkTitle": "Engage",
                "chunkStart": 0,
                "chunkDur": 10,
                "steps": [
                  {
                    "Step": 1,
                    "StepTitle": "What is a Mind Map?",
                    "StepQuickDescription": "Through examples and a supporting video, students will learn what a mind map is and how it connects to the topic of water.",
                    "StepDetails": "The lesson 1 slideshow will walk you through the entire lesson. Additionally, the teacher worksheet has several mind-mapping tips.",
                    "Vocab": "- **Mind map:**  a diagram that visually organizes information to show relationships among connected concepts.",
                    "VariantNotes": null,
                    "TeachingTips": null
                  }
                ]
              },
              {
                "chunkTitle": "Explore",
                "chunkStart": 10,
                "chunkDur": 10,
                "steps": [
                  {
                    "Step": 2,
                    "StepTitle": "Create Your Mind Map",
                    "StepQuickDescription": "Students will create a mind map that represents their connection to water.",
                    "StepDetails": "Questions to prompt student thinking:\n- How do you use water daily?\n- What aspects of your life rely on clean water?\n- How does your family / community rely on water?",
                    "Vocab": null,
                    "VariantNotes": null,
                    "TeachingTips": "This fun and creative exercise would be further supported if teachers tried it for themselves ahead of time!"
                  }
                ]
